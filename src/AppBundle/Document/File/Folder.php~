namespace AppBundle\Document\File;
use Doctrine\ODM\MongoDB\Mapping\Annotations as MongoDB;

/**
 * @MongoDB\Document
 */
class Folder extends File{

    /**
     * @MongoDB\ReferenceMany(targetDocument="File")
     */
    public $children;


}